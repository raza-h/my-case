@model AbsolCase.Models.UserTitle
@{
    ViewBag.Title = "Update User Title";
    Layout = "~/Views/Shared/_SPLayout.cshtml";
}
@if (TempData["response"] != null)
{
    <div class="alert-danger" id="msgalert">
        @TempData["response"].ToString()
    </div>
}
<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Add User Title</h4>
            <form asp-action="AddPracticeArea" method="post">
                <input type="hidden" value="@Model.Id" />
                <div class="form-group">
                    <label for="exampleInputUsername1">User Title <span style="font-weight:bolder;font-size:18px;color:red">*</span></label>
                    @Html.EditorFor(model => model.UserTitleName, new { htmlAttributes = new { @class = "form-control", placeholder = "User Title" } })
                    @Html.ValidationMessageFor(model => model.UserTitleName, "", new { @class = "text-danger db" })
                </div>
                <input type="submit" value="Update" class="btn btn-dark fr" />
                @*<button class="btn btn-light">Cancel</button>*@
            </form>
        </div>
    </div>
</div>
<script>
    setTimeout(function () {
        $("#msgalert").hide();
    }, 3500);
</script>